{
  "name": "patryqhyper/laravel-translation-checker",
  "description": "Library to show all missing translations within your Laravel project",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Patryk Vizauer",
      "email": "patryk@vizauer.pl"
    }
  ],
  "require": {
    "php": "^8.3",
    "spatie/laravel-package-tools": "^1.92",
    "laravel/framework": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "PatryQHyper\\LaravelTranslationChecker\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "PatryQHyper\\LaravelTranslationChecker\\LaravelTranslationCheckerServiceProvider"
      ]
    }
  }
}
